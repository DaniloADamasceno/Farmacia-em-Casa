<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#141c34"
    tools:context=".MainActivity">

    <!--    imagem do Logo da FarmÃ¡cia-->
    <ImageView
        android:id="@+id/logoFarmacia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="54dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toTopOf="@+id/androidLogo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_farmacia"
        tools:ignore="ContentDescription,MissingConstraints" />

<!--    Logo Android-->
    <ImageView
        android:id="@+id/androidLogo"
        android:layout_width="334dp"
        android:layout_height="129dp"
        android:layout_marginStart="44dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="41dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logoFarmacia"
        app:srcCompat="@drawable/android_white_png"
        tools:ignore="ContentDescription" />
</androidx.constraintlayout.widget.ConstraintLayout>